{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.py"
    }
  ],
  "env": {
    "OMNISCIENT_API_HOST": "0.0.0.0",
    "OMNISCIENT_API_PORT": "8000",
    "OMNISCIENT_DEBUG": "false"
  }
}
